[run]
branch = True

[report]
omit =
    django_route/migrations/*
    django_route/admin.py
    django_route/signals/*
    django_route/conf/*
exclude_lines =
    pragma: no cover
    def __str__
    def __unicode__
    def __repr__
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
